			<pointer>b3</pointer>
			<pointer>b4</pointer>
			<pointer>b5</pointer>
			<action></action>
		</dialogue>
		<dialogue>
			<id>b2</id>
			<prerequisites></prerequisites>
			<text>Must I go alone?</text>
